<div class="container">

 <div class="row">
                <div class="col-md-6">
                    <!-- Form Elements -->
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Register User
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-6">
                           
                                  <form id="uploadForm" enctype="multipart/form-data" action="/pictureUpload" method="post">
                                    <div class="form-group">
                                      <input type="hidden" class="form-control" id="id"  name="id" value="{{ id }}">
                                      <label for="pwd">Image Name:</label>
                                      <input type="text" class="form-control" id="name" placeholder="Enter password" name="name">
                                    </div>
                                    <div class="form-group">
                                      <label for="email">File Upload:</label>
                                      <input  class="form-control" type="file" name="userPhoto" multiple />
                                    </div>
                                    <div class="form-group">
                                       <input type="submit" class="btn btn-primary" value="Upload Image" name="submit">
                                    </div>
                                </form>
                                </div>
                            </div>
                        </div>
                    </div>
                     <!-- End Form Elements -->
                </div>
            </div>
                <!-- /. ROW  -->


                <div class="row">
                <div class="col-md-12">
                    <!-- Advanced Tables -->
                    <div class="panel panel-default">
                        <div class="panel-heading">
                             Gallery Tables
                        </div>
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        <tr>

                                            <th>File Name</th>
                                            <th>Image</th>
                                                                                      
                                            <th>Action </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{# each item }}
                                          <tr class="odd gradeX">
                                              <td> <img src="C:\Users\Riya\Desktop\nodejs\nodejs_test\Images\"{{ this.name }} /></td>
                                              <td>{{ this.name }}</td>                                             
                                              
                                             <td><a href="/fsafa/{{ this._id }}" class="btn btn-primary btn-xs editbtn" title="edit">Edit</a>
                                            <a class="btn btn-danger btn-xs" href="/delete_user/{{ this._id }}" title="delete">Delete</a></td>
                                          </tr>
                                      {{/each}}
                                        
                                    </tbody>
                                </table>
                            </div>
                            
                        </div>
                    </div>
                    <!--End Advanced Tables -->
                </div>
            </div>
</div>