<div class="container">

   <div class="row">
                <div class="col-md-12">
                    <!-- Advanced Tables -->
                    <div class="panel panel-default">
                        <div class="panel-heading">
                             Advanced Tables
                        </div>
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>Id</th>
                                            <th>Name</th>
                                            <th>Email</th>
                                            <th>Contact</th>
                                            <th>Upload Image</th>                                            
                                            <th>Action </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{# each items }}
                                          <tr class="odd gradeX">
                                              <td> {{ this._id }}</td>
                                              <td>{{ this.userName }}</td>
                                              <td>{{ this.email }}</td>
                                              <td>{{ this.contact }}</td>
                                              <td><a href="/gallery/{{ this._id }}"> Open Gallery </a></td>
                                              <td><a href="/edit_user/{{ this._id }}" class="btn btn-primary btn-xs editbtn" title="edit">Edit</a>
                                            <a class="btn btn-danger btn-xs" href="/delete_user/{{ this._id }}" title="delete">Delete</a></td>
                                          </tr>
                                      {{/each}}
                                        
                                    </tbody>
                                </table>
                            </div>
                            
                        </div>
                    </div>
                    <!--End Advanced Tables -->
                </div>
            </div>
</div>